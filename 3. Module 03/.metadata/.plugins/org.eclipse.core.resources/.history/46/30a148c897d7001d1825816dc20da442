package com.ust.ioc2;

public class Line {
	
	private int p1;
	private int p2;
	
	
	
	public int getP1() {
		return p1;
	}



	public void setP1(int p1) {
		this.p1 = p1;
	}



	public int getP2() {
		return p2;
	}



	public void setP2(int p2) {
		this.p2 = p2;
	}



	public void DrawLine() {
		System.out.println("from" +p1.+ ","+p1.getY());
		System.out.println("from" +p2.getX()+ "," +p2.getY());

	}

}
